{
  "name": "sort-object-selection",
  "displayName": "Sort Object Selection",
  "description": "Sort an entire object or selection of object key pairs in ascending or descending order.",
  "version": "0.0.1",
  "publisher": "sortObjectSelection",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sortObjectSelection.sortObjectSelectionAscending",
        "title": "Sort Object Selection (Ascending)"
      },
      {
        "command": "sortObjectSelection.sortObjectSelectionDescending",
        "title": "Sort Object Selection (Descending)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "sortObjectSelection.sortObjectSelectionAscending",
          "group": "1_modification"
        },
        {
          "when": "editorHasSelection",
          "command": "sortObjectSelection.sortObjectSelectionDescending",
          "group": "1_modification"
        }
      ]
    },
    "keybindings": [
      {
        "command": "sortObjectSelection.sortObjectSelectionAscending",
        "key": "ctrl+shift+alt+u",
        "mac": "cmd+shift+alt+u",
        "when": "editorTextFocus"
      },
      {
        "command": "sortObjectSelection.sortObjectSelectionDescending",
        "key": "ctrl+shift+alt+d",
        "mac": "cmd+shift+alt+d",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "mocha"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@types/mocha": "^10.0.7",
    "@vscode/test-cli": "^0.0.9",
    "eslint": "^8.57.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "mocha": "^10.5.2",
    "vscode": "^1.1.37"
  }
}